    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-fastify": "^10.2.7",
    "fastify": "^4.24.3",
    "nest-winston": "^1.9.4",
    "reflect-metadata": "^0.1.13",
